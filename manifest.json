{
  "manifest_version": 3,
  "name": "Creamy/Thock Key Clicks",
  "version": "3.8",
  "description": "Creaky Keyboard with volume HUD on all websites",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
